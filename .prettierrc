{
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "htmlWhitespaceSensitivity": "strict",
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    }
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/screens/(.*)$",
    "^@/hooks/(.*)$",
    "^@/shared/(.*)$",
    "^@/assets/(.*)$",
    "^@/utils/(.*)$",
    "^@/types/(.*)$",
    "^@/configs/(.*)$",
    "^@/store/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "tailwindFunctions": ["clsx"]
}
